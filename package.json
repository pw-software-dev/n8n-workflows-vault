{
  "name": "n8n-workflows",
  "version": "1.0.0",
  "description": "Structured repository for managing n8n workflows with validation and automation",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "validate-all-workflows": "ts-node scripts/validate-workflow.ts",
    "check-metadata-consistency": "ts-node scripts/validate-workflow.ts --check-consistency",
    "validate-readme-completeness": "ts-node scripts/validate-workflow.ts --check-readme",
    "generate-readme": "ts-node scripts/generate-readme.ts",
    "generate-documentation": "ts-node scripts/generate-documentation.ts",
    "migrate-workflow": "ts-node scripts/migrate-workflow.ts",
    "test": "npm run validate-all-workflows",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.23",
    "ai": "^5.0.29",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "dotenv": "^17.2.1"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "jest": "^29.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "keywords": [
    "n8n",
    "workflows",
    "automation",
    "validation"
  ],
  "author": "",
  "license": "MIT"
}
